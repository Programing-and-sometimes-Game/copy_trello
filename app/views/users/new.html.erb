<h1>Users#new</h1>
<%= form_for(@user, url: signup_path) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :mail %>
	<%= f.email_field :mail %>
	<%= f.label :password %>
	<%= f.password_field :password %>
	<%= f.submit "ログイン" %>
<% end %>

<%= link_to 'Loginページ', login_path %>
